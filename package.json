{
   "name": "example",
   "version": "1.2.0",
   "description": "Example usage of sqljson-query to generate SQL queries and TypeScript result types.",
   "repository": {},
   "keywords": [
      "SQL",
      "SQL/JSON",
      "TypeScript"
   ],
   "author": "Stephen C. Harris",
   "license": "MIT",
   "scripts": {
      "build-db": "docker build -t drugs-pg db",
      "start-db": "npm run build-db && docker run -d --name drugs-pg --rm -p 127.0.0.1:5432:5432 --shm-size=256MB drugs-pg",
      "psql": "docker exec -it drugs-pg psql -U drugs",
      "gen-dbmd": "node query-gen/js/gen-dbmd.js --jdbcProps=db/jdbc.props --db=pg",
      "gen-app-relsmd": "node query-gen/js/gen-relsmd.js --tsRelsMdDir=src/generated/lib",
      "pregen-queries-java": "tsc query-gen/query-specs.ts",
      "gen-queries-java": "node query-gen/js/gen-queries.js --sqlDir=src/generated/sql --javaBaseDir=src/generated/lib --javaQueriesPkg=javapkg",
      "pregen-queries-ts": "tsc query-gen/query-specs.ts",
      "gen-queries-ts": "node query-gen/js/gen-queries.js --sqlDir=src/generated/sql --tsQueriesDir=src/generated/lib",
      "prebuild": "npm run gen-queries-ts",
      "build": "npm run clean && tsc && copyfiles --flat src/generated/sql/*.sql dist/generated/sql",
      "preapp": "npm run build",
      "app": "node dist/app.js dist/generated/sql db/connection.env",
      "clean": "rimraf dist"
   },
   "dependencies": {
      "dotenv": "^8.6.0",
      "pg": "^8.7.1",
      "pg-pool": "^3.4.1"
   },
   "devDependencies": {
      "@types/node": "^14.18.0",
      "@types/pg": "^7.14.11",
      "@types/pg-pool": "^2.0.3",
      "copyfiles": "^2.4.1",
      "rimraf": "^3.0.2",
      "typescript": "^4.5.2"
   }
}
