{
   "name": "example",
   "version": "1.1.0",
   "description": "Example usage of sqljson-query to generate SQL queries and TypeScript result types.",
   "repository": {},
   "keywords": [
      "SQL",
      "SQL/JSON",
      "TypeScript"
   ],
   "author": "Stephen C. Harris",
   "license": "MIT",
   "scripts": {
      "build-db": "docker build -t drugs-pg db",
      "start-db": "npm run build-db && docker run -d --name drugs-pg --rm -p 127.0.0.1:5432:5432 --shm-size=256MB drugs-pg",
      "psql": "docker exec -it drugs-pg psql -U drugs",
      "gen-dbmd": "java -jar query-gen/dbmd/target/dbmd-fetcher.jar db/jdbc.props pg '.*' query-gen/dbmd/dbmd.json",
      "gen-queries-java": "npm run --prefix query-gen generate-queries -- --sqlDir=../src/generated/sql --javaBaseDir=../src/generated/lib --javaPkg=javapkg",
      "gen-queries-ts": "npm run --prefix query-gen generate-queries -- --sqlDir=../src/generated/sql --tsDir=../src/generated/lib --tsTypesHeader=queries/result-types-header-ts",
      "prebuild": "npm run gen-queries-ts && npm run gen-queries-java",
      "build": "npm run clean && tsc && copyfiles --flat src/generated/sql/*.sql dist/generated/sql",
      "preapp": "npm run build",
      "app": "node dist/app.js dist/generated/sql db/connection.env",
      "clean": "rimraf dist"
   },
   "dependencies": {
      "dotenv": "^8.6.0",
      "pg": "^8.6.0",
      "pg-pool": "^3.3.0"
   },
   "devDependencies": {
      "@types/node": "^14.17.0",
      "@types/pg": "^7.14.11",
      "@types/pg-pool": "^2.0.2",
      "copyfiles": "^2.4.1",
      "rimraf": "^3.0.2",
      "typescript": "^4.2.4"
   }
}
