{
   "name": "example",
   "version": "1.0.0",
   "description": "Example usage of sqljson-query to generate SQL queries and TypeScript result types.",
   "repository": {},
   "keywords": [
      "SQL",
      "SQL/JSON",
      "TypeScript"
   ],
   "author": "Stephen C. Harris",
   "license": "MIT",
   "scripts": {
      "build-db": "docker build -t drugs-pg db",
      "start-db": "npm run build-db && docker run -d --name drugs-pg --rm -p 127.0.0.1:5432:5432 --shm-size=256MB drugs-pg",
      "psql": "docker exec -it drugs-pg psql -U drugs",
      "generate-dbmd": "dbmd-gen-pg --conn-env db/connection.env query-gen/dbmd.json",
      "compile-qspecs": "cd query-gen && tsc --strict query-specs.ts",
      "sjq-gen-ts": "sqljson-query-gen --dbmd ./query-gen/dbmd.json --query-specs ./query-gen/query-specs.js --types-header query-gen/types-header-ts --src-output-dir src/generated/lib --sql-output-dir src/generated/sql",
      "sjq-gen-java": "sqljson-query-gen --dbmd ./query-gen/dbmd.json --query-specs ./query-gen/query-specs.js --types-header query-gen/types-header-java --src-lang Java --src-output-dir src/generated/lib/javapkg --sql-output-dir src/generated/sql",
      "prebuild": "npm run compile-qspecs && npm run sjq-gen-ts && npm run sjq-gen-java",
      "build": "npm run clean && tsc && copyfiles --flat src/generated/sql/*.sql dist/generated/sql",
      "preapp": "npm run build",
      "app": "node dist/app.js dist/generated/sql db/connection.env",
      "clean": "rimraf dist"
   },
   "dependencies": {
      "dotenv": "^8.6.0",
      "pg": "^8.6.0",
      "pg-pool": "^3.3.0"
   },
   "devDependencies": {
      "@types/node": "^14.17.0",
      "@types/pg": "^7.14.11",
      "@types/pg-pool": "^2.0.2",
      "copyfiles": "^2.4.1",
      "dbmd-pg": "^1.1.1",
      "rimraf": "^3.0.2",
      "typescript": "^4.2.4",
      "sqljson-query": "1.0.11"
   }
}
